@model IEnumerable<DtdcCashCounter.EntityFr.wallet_History>

@{
    ViewBag.Title = "WalletHistory";
    Layout = "~/Views/Shared/CashCounterLayout.cshtml";
}



<div class="content-wrapper">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.H_Status)
            </th>
         
            <th>
                @Html.DisplayNameFor(model => model.consignment_no)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Amount)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.mobile_no)
