                @Html.DisplayFor(modelItem => item.Amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.mobile_no)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.datetime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Franchisee.F_Address)
            </td>
         
            <td>
                @*@Html.ActionLink("Edit", "Edit", new { id = item.H_Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.H_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.H_Id })*@
