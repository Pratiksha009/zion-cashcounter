@model IEnumerable<DtdcCashCounter.EntityFr.Expense>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/CashCounterLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rason)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Category)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Franchisee.F_Address)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rason)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Category)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Franchisee.F_Address)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Exp_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Exp_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Exp_ID })
        </td>
    </tr>
}

</table>
